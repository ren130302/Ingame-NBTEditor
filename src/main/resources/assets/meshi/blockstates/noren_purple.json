{
    "multipart": [
        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "meshi:block/noren_purple_post" }
        },
        {   "when": { "north": true },
            "apply": { "model": "meshi:block/noren_purple_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "meshi:block/noren_purple_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "meshi:block/noren_purple_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "meshi:block/noren_purple_side_alt", "y": 90 }
        }
    ]
}
