        {   "when": { "north": true, "east": true, "south": false, "west": true },
            "apply": { "model": "meshi:block/kotatsu_convex_n" }
        },
        {   "when": { "north": false, "east": true, "south": true, "west": true },
            "apply": { "model": "meshi:block/kotatsu_convex_s" }
